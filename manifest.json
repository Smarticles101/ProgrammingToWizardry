{
  "manifest_version": 2,

  "name": "Programming to Wizardry",
  "description": "Replaces words about programming with words about wizardry",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}